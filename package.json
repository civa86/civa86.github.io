    "moment": "2.29.2",
    "node-sass": "7.0.0",
    "octicons": "4.3.0",
    "prettier": "2.2.1",
    "react": "17.0.1",
    "react-awesome-tabs": "0.1.0",
    "react-dom": "17.0.1",
    "react-image-gallery": "1.0.9",
